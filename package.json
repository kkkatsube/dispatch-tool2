{
  "name": "dispatch-tool2",
  "version": "1.0.0",
  "description": "Google Sheets dispatch tool with TypeScript and clasp",
  "main": "dist/Code.js",
  "scripts": {
    "build": "tsc && cp appsscript.json dist/",
    "watch": "tsc --watch",
    "push": "npm run build && clasp push --force",
    "deploy": "npm run build && clasp push --force && clasp deploy",
    "open": "clasp open"
  },
  "keywords": [
    "google-apps-script",
    "typescript",
    "clasp",
    "spreadsheet"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@types/google-apps-script": "^1.0.83",
    "typescript": "^5.3.3"
  }
}
